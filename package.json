    "@prisma/client": "^6.18.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "i18next": "^25.6.0",
    "i18next-express-middleware": "^2.0.0",
    "i18next-fs-backend": "^2.6.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "node-fetch": "^3.3.2",
    "svix": "^1.77.0",
    "winston": "^3.18.3"
