    "@types/i18next-fs-backend": "^1.1.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.5",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "eslint": "^9.18.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.4.2",
    "prisma": "^6.18.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
